{
  "manifest_version": 3,
  "name": "METAR lookup",
  "description": "A tool to look up weather at airports",
  "version": "1.0",
  "permissions": [
     "activeTab"
   ],
   "host_permissions": [
      "<all_urls>"
   ],
  "action": {
    "default_popup": "index.html",
    "default_icon": "vite.svg"
  }
}