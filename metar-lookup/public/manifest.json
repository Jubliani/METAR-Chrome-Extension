{
    "manifest_version": 3,
    "name": "METAR lookup1",
    "description": "A tool to look up weather at airports",
    "version": "1.0",
    "host_permissions": [
      "http://*",
      "https://*"
    ],
    "permissions": [
        "scripting"
    ],
    "action": {
      "default_popup": "index.html",
      "default_icon": "vite.svg"
    }
  }